{
  "name": "inversime",
  "description": "Simple reactive library",
  "version": "1.0.1",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/inversime.umd.cjs",
  "module": "./dist/inversime.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/inversime.js",
      "require": "./dist/inversime.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evuz/inversime.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "lint": "eslint src/**",
    "lint:format": "eslint --fix src/**",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "type:check": "tsc",
    "type:check:watch": "tsc --watch"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "eslint": "8.36.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-promise": "6.1.1",
    "typescript": "5.0.2",
    "vite": "4.2.1",
    "vitest": "0.29.7"
  }
}
